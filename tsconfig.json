{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es2017",                                
    "module": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "typeRoots": ["./src/utils"],
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,               
    "skipLibCheck": true,
    "noImplicitAny": false,
  }
}